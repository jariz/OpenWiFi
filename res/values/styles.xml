<resources>

    <style name="OpenWiFi" parent="android:Theme.Holo">
        <item name="circularProgressBarStyle">@style/CircularProgressBar</item>

    </style>

    <style name="OpenWiFi.Circular" parent="CircularProgressBar">
        <item name="progress" format="float">1</item>
    </style>

    <style name="OpenWiFi.Circular.Green" parent="OpenWiFi.Circular">
        <item name="progress_color">@android:color/holo_green_light</item>
    </style>

    <style name="OpenWiFi.Circular.Blue" parent="OpenWiFi.Circular">
        <item name="progress_color">@android:color/holo_blue_bright</item>
    </style>

    <style name="AppTheme" parent="OpenWiFi">
    </style>

</resources>
